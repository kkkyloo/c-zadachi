static void Main(string[] args)
{
   int n = int.Parse(Console.ReadLine());
   int i;
   int a;
   int b;
   string c;
   for (i = 1; i <= n; i++)
   {
      c = null;
      for (a = 1; a <= n; a++)
      {
         b = i % a;
         if (b == 0)
            c += "+";
      }
      Console.WriteLine(i + c);
   }
}
