Вычислить факториал числа p(при p > 0), используя цикл do while.

using System;

namespace ConsoleApp5
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int p;
            int fakt = 1;
            int i = 1;
            p = Convert.ToInt32(Console.ReadLine());
            if (p > 0)
            {
                do
                {
                    fakt *= i;
                    i++;
                }
                while (i <= p);
                Console.WriteLine(fakt);
                Console.ReadLine();
            }
        }
    }
}
